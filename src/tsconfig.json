{
	"compilerOptions": {
		"target": "ES2017",
		"watch": true,
		"lib": ["dom"],
		"allowUnreachableCode": true,
		"rootDir": "/home/sander/Documents/Repositoray/Bedriftregister/src",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
